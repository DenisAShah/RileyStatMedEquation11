Cell[BoxData[
 FractionBox[
  RowBox[{
   RowBox[{"(", 
    RowBox[{"1", "-", "Cs"}], ")"}], " ", "Cs", " ", 
   RowBox[{"(", 
    RowBox[{"1", "+", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", "Cs"}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", 
         FractionBox["Ns", "2"]}], ")"}]}], 
      RowBox[{"2", "-", "Cs"}]], "+", 
     FractionBox[
      RowBox[{"Cs", " ", 
